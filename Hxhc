local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Hotel Elephant", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"}) 

OrionLib:MakeNotification({
	Name = "Credits",
	Content = "Made by wesze_kluzek",
	Image = "rbxassetid://4483345998",
	Time = 5
})

local MainTab = Window:MakeTab({
	Name = "Auto",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

MainTab:AddButton({
	Name = "Give 1B Money",
	Callback = function()
game:GetService("ReplicatedStorage").MoneyRequest:FireServer(false,999999999,"Cash")
  	end    
})

MainTab:AddToggle({
	Name = "AutoFarm Money",
	Default = false,
	Callback = function(Value)
		_G.Toggle = Value

while _G.Toggle do
wait()
game:GetService("ReplicatedStorage").MoneyRequest:FireServer(false,999999999,"Cash")
end
	end    
})

MainTab:AddSlider({
	Name = "Walkspeed",
	Min = 0,
	Max = 20,
	Default = 5,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Walkspeed",
	Callback = function(Value)
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
	end    
})

MainTab:AddTextbox({
	Name = "Walkspeed (mobile)",
	Default = "Change Walkspeed",
	TextDisappear = true,
	Callback = function(Value)
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
	end	  
})

MainTab:AddColorpicker({
	Name = "ColorPicker",
	Default = Color3.fromRGB(255, 0, 0),
	Callback = function(Value)
		print(Value)
	end	  
})

OrionLib:Init()
